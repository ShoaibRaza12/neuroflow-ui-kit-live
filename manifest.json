{
  "name": "NeuroFlow UI Kit - Premium AI Dashboard Components",
  "short_name": "NeuroFlow UI",
  "description": "A futuristic SaaS admin dashboard UI kit with glassmorphism, neumorphic design, and AI-powered components.",
  "version": "1.0.0",
  "manifest_version": 2,
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0a0b1e",
  "theme_color": "#00d4ff",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "categories": ["productivity", "business", "developer"],
  "screenshots": [
    {
      "src": "/screenshots/desktop-1.png",
      "type": "image/png",
      "sizes": "1280x720",
      "form_factor": "wide",
      "label": "NeuroFlow Dashboard Overview"
    },
    {
      "src": "/screenshots/mobile-1.png",
      "type": "image/png", 
      "sizes": "375x667",
      "form_factor": "narrow",
      "label": "NeuroFlow Mobile Dashboard"
    }
  ],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard Overview",
      "short_name": "Overview",
      "description": "View AI-powered dashboard insights",
      "url": "/?page=overview",
      "icons": [{ "src": "/icons/shortcut-overview.png", "sizes": "96x96" }]
    },
    {
      "name": "Workflow Builder",
      "short_name": "Workflows",
      "description": "Create and manage AI workflows",
      "url": "/?page=workflow",
      "icons": [{ "src": "/icons/shortcut-workflow.png", "sizes": "96x96" }]
    },
    {
      "name": "Task Stream",
      "short_name": "Tasks",
      "description": "Monitor real-time task updates",
      "url": "/?page=tasks", 
      "icons": [{ "src": "/icons/shortcut-tasks.png", "sizes": "96x96" }]
    },
    {
      "name": "Team Analytics",
      "short_name": "Analytics",
      "description": "View team performance metrics",
      "url": "/?page=analytics",
      "icons": [{ "src": "/icons/shortcut-analytics.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "scope_extensions": [
    {
      "origin": "https://neuroflow-ui-kit.demo"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+neuroflow",
      "url": "/?action=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", ".csv", ".json"]
        }
      ]
    }
  }
}